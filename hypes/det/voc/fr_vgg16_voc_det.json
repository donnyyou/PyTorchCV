{
    "dataset": "voc",
    "task": "det",
    "method": "faster_rcnn",
    "data": {
      "train_batch_size": 16,
      "val_batch_size": 2,
      "train_input_size": [720, 720],
      "val_input_size": [720, 720],
      "num_classes": 21,
      "data_dir": "/home/donny/DataSet/VOC2007_DET",
      "workers": 8
    },
    "test": {
      "test_input_size": [720, 720],
      "scale_search": [0.5, 1.0, 1.5, 2.0],
      "mirror": true,
      "crop_test": false,
      "crop_size": [864, 864],
      "crop_stride_ratio": 0.667
    },
    "train_trans": {
      "pad_ratio": 0.0,
      "brightness_ratio": 0.0,
      "blur_ratio": 0.0,
      "hsv_ratio": 0.0,
      "flip_ratio": 0.0,
      "resize_ratio": 0.0,
      "rotate_ratio": 0.0,
      "crop_ratio": 0.0,
      "trans_seq": ["random_brightness", "random_gauss_blur", "random_hsv",
                    "random_hflip", "random_resize", "random_rotate",
                    "random_crop", "resize"]
    },
    "val_trans": {
      "pad_ratio":0.0,
      "brightness_ratio": 0.0,
      "blur_ratio": 0.0,
      "hsv_ratio": 0.0,
      "flip_ratio": 0.0,
      "resize_ratio": 0.0,
      "rotate_ratio": 0.0,
      "crop_ratio": 0.0,
      "trans_seq": ["random_brightness", "random_gauss_blur", "random_hsv",
                    "random_hflip", "random_resize", "random_rotate",
                    "random_crop", "resize"]
    },
    "trans_params": {
      "random_pad":{
        "pad_border": 2
      },
      "random_brightness": {
        "shift_value": 30
      },
      "random_gauss_blur": {
        "max_blur": 4
      },
      "random_hsv": {
        "h_range": [1.0, 1.0],
        "s_range": [0.7, 1.3],
        "v_range": [0.7, 1.3]
      },
      "random_hflip": {
        "swap_pair": []
      },
      "random_resize": {
        "scale_min": 0.75,
        "scale_max": 1.25
      },
      "random_rotate": {
        "rotate_degree": 40
      },
      "random_crop":{
        "crop_size": [1024, 512],
        "method": "grid",
        "grid":[3, 2],
        "center_jitter": 20
      },
      "mean": [0.485, 0.456, 0.406],
      "std": [0.229, 0.224, 0.225]
    },
    "rpn": {
      "anchor_sizes_list": [
        [[128, 128], [256, 256], [512, 512]]
      ],
      "num_anchor_list": [9],
      "aspect_ratio_list": [[2]],
      "stride_list": [16],
      "nms_threshold": 0.7,
      "min_size": 16,
      "loss": {
        "n_sample": 256,
        "pos_ratio": 0.5,
        "pos_iou_thresh": 0.7,
        "neg_iou_thresh": 0.3
      },
      "n_train_pre_nms": 12000,
      "n_train_post_nms": 2000,
      "n_test_pre_nms": 6000,
      "n_test_post_nms": 300

    },
    "roi": {
      "pooled_width": 7,
      "pooled_height": 7,
      "spatial_stride": 16,
      "loc_normalize_mean": [0.0, 0.0, 0.0, 0.0],
      "loc_normalize_std": [0.1, 0.1, 0.2, 0.2],
      "loss": {
        "n_sample": 128,
        "pos_ratio": 0.25,
        "pos_iou_thresh": 0.5,
        "neg_iou_thresh_hi": 0.5,
        "neg_iou_thresh_lo": 0.0
      }
    },
    "details": {
      "color_list": [[255, 170, 30], [0, 0, 70], [244, 35, 232]],
      "name_id_dict": {
        "aeroplane": 1, "bicycle": 2, "bird": 3, "boat": 4, "bottle": 5, "bus": 6, "car": 7,
        "cat": 8, "chair": 9, "cow": 10, "diningtable": 11, "dog": 12, "horse": 13, "motorbike": 14,
        "person": 15, "pottedplant": 16, "sheep": 17, "sofa": 18, "train": 19, "tvmonitor": 20
      },
      "name_seq": ["aeroplane", "bicycle", "bird", "boat", "bottle", "bus", "car", "cat", "chair",
                   "cow", "diningtable", "dog", "horse", "motorbike", "person", "pottedplant",
                   "sheep", "sofa", "train", "tvmonitor"]
    },
    "checkpoints": {
      "checkpoints_name": "fr_vgg16_voc_det",
      "checkpoints_dir": "checkpoints/det/voc",
      "save_iters": 3000
    },
    "network":{
      "backbone": "vgg16",
      "model_name": "faster_rcnn",
      "init": "kaiming_normal",
      "loss_weights": {
        "rpn_loss": 3.0,
        "roi_loss": 1.0
      }
    },
    "logging": {
      "logfile_level": "info",
      "stdout_level": "info",
      "log_file": "./log/det/fr_vgg16_voc_det.log",
      "log_format": "%(asctime)s %(levelname)-7s %(message)s",
      "rewrite": true
    },
    "lr": {
      "base_lr": 0.001,
      "lr_policy": "step",
      "step": {
        "gamma": 0.1,
        "step_size": 20
      }
    },
    "solver": {
      "display_iter": 100,
      "test_interval": 1000,
      "max_epoch": 100
    },
    "optim": {
      "optim_method": "sgd",
      "adam": {
        "betas": [0.9, 0.999],
        "eps": 1e-08,
        "weight_decay": 0.0001
      },
      "sgd":{
        "weight_decay": 0.0005,
        "momentum": 0.9
      }
    },
    "nms": {
      "mode": "union",
      "overlap_threshold": 0.3
    },
    "vis": {
      "conf_threshold": 0.02
    },
    "fr_loss": {
      "rpn_sigma": 3.0,
      "roi_sigma": 1.0
    }
}