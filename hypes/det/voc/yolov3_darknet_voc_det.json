{
    "dataset": "voc",
    "task": "det",
    "method": "yolov3",
    "data": {
      "num_classes": 20,
      "data_dir": "/home/donny/DataSet/VOC07+12_DET",
      "image_tool": "pil",
      "input_mode": "RGB",
      "keep_difficult": false,
      "workers": 8
    },
    "train": {
      "batch_size": 16,
      "data_transformer": {
        "size_mode": "fix_size",
        "input_size": [416, 416],
        "align_method": "only_scale"
      }
    },
    "val": {
      "batch_size": 16,
      "data_transformer": {
        "size_mode": "fix_size",
        "input_size": [416, 416],
        "align_method": "only_scale"
      }
    },
    "test": {
      "input_size": [416, 416],
      "scale_search": [1.0]
    },
    "train_trans": {
      "saturation_ratio": 0.5,
      "hue_ratio": 0.5,
      "perm_ratio": 0.5,
      "contrast_ratio": 0.5,
      "pad_ratio":0.5,
      "brightness_ratio": 0.5,
      "blur_ratio": 0.5,
      "flip_ratio": 0.5,
      "crop_ratio": 1.0,
      "trans_seq": ["random_contrast", "random_hue", "random_saturation", "random_brightness",
                    "random_gauss_blur", "random_perm", "random_hflip", "random_pad", "random_crop"]
    },
    "val_trans": {
      "trans_seq": []
    },
    "trans_params": {
      "random_saturation": {
        "lower": 0.5,
        "upper": 1.5
      },
      "random_hue": {
        "delta": 18
      },
      "random_contrast": {
        "lower": 0.5,
        "upper": 1.5
      },
      "random_pad": {
        "up_scale_range": [1.0, 4.0]
      },
      "random_brightness": {
        "shift_value": 32
      },
      "random_gauss_blur": {
        "max_blur": 4
      },
      "random_hflip": {
        "swap_pair": []
      },
      "random_crop":{
        "crop_size": [1024, 512],
        "method": "grid",
        "grid":[3, 2],
        "center_jitter": 20
      },
      "normalize": {
        "div_value": 255,
        "mean_value": [124, 116, 104],
        "mean": [0.485, 0.456, 0.406],
        "std": [0.229, 0.224, 0.225]
      }
    },
    "gt": {
      "anchors_list": [
        [[116, 90], [156, 198], [373, 326]],
        [[30, 61], [62, 45], [59, 119]],
        [[10, 13], [16, 30], [33, 23]]
      ],
      "iou_threshold": 0.5
    },
    "details": {
      "color_list": [[255, 170, 30], [0, 0, 70], [244, 35, 232]],
      "name_id_dict": {
        "aeroplane": 1, "bicycle": 2, "bird": 3, "boat": 4, "bottle": 5, "bus": 6, "car": 7,
        "cat": 8, "chair": 9, "cow": 10, "diningtable": 11, "dog": 12, "horse": 13, "motorbike": 14,
        "person": 15, "pottedplant": 16, "sheep": 17, "sofa": 18, "train": 19, "tvmonitor": 20
      },
      "name_seq": ["aeroplane", "bicycle", "bird", "boat", "bottle", "bus", "car", "cat", "chair",
                   "cow", "diningtable", "dog", "horse", "motorbike", "person", "pottedplant",
                   "sheep", "sofa", "train", "tvmonitor"]
    },
    "checkpoints": {
      "checkpoints_name": "yolov3_darknet_voc_det",
      "checkpoints_dir": "checkpoints/det/voc",
      "save_iters": 3000
    },
    "network":{
      "backbone": "darknet53",
      "model_name": "darknet_yolov3",
      "stride_list": [32, 16, 8],
      "loss_weights": {
        "coord_loss": 2.5,
        "obj_loss": 1.0,
        "cls_loss": 1.0
      }
    },
    "logging": {
      "logfile_level": "info",
      "stdout_level": "info",
      "log_file": "./log/det/yolov3_darknet_voc_det.log",
      "log_format": "%(asctime)s %(levelname)-7s %(message)s",
      "rewrite": true
    },
    "lr": {
      "base_lr": 0.001,
      "lr_policy": "multistep",
      "step": {
        "gamma": 0.1,
        "step_size": 500
      },
      "multistep": {
        "gamma": 0.1,
        "stepvalue": [100, 120]
      }
    },
    "solver": {
      "display_iter": 100,
      "test_interval": 3000,
      "max_epoch": 150
    },
    "optim": {
      "optim_method": "sgd",
      "adam": {
        "betas": [0.9, 0.999],
        "eps": 1e-08,
        "weight_decay": 0.0001
      },
      "sgd":{
        "weight_decay": 0.00004,
        "momentum": 0.9
      }
    },
    "nms": {
      "mode": "union",
      "max_threshold": 0.4
    },
    "vis": {
      "conf_threshold": 0.2
    }
}
