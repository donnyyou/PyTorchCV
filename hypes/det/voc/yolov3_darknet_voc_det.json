{
    "dataset": "voc",
    "task": "det",
    "method": "yolov3",
    "data": {
      "train_batch_size": 16,
      "val_batch_size": 2,
      "train_input_size": [416, 416],
      "val_input_size": [416, 416],
      "num_classes": 20,
      "data_dir": "/home/donny/DataSet/VOC2007_DET",
      "workers": 8
    },
    "test": {
      "test_input_size": [416, 416],
      "scale_search": [0.5, 1.0, 1.5, 2.0],
      "mirror": true
    },
    "train_trans": {
      "pad_ratio": 0.0,
      "shift_ratio": 0.0,
      "brightness_ratio": 0.0,
      "blur_ratio": 0.0,
      "hsv_ratio": 0.0,
      "flip_ratio": 0.0,
      "resize_ratio": 0.0,
      "rotate_ratio": 0.0,
      "crop_ratio": 0.0,
      "det_crop_ratio": 0.0,
      "trans_seq": ["random_brightness", "random_gauss_blur", "random_hsv",
                    "random_hflip", "random_resize", "random_rotate",
                    "random_crop", "resize"]
    },
    "val_trans": {
      "pad_ratio":0.0,
      "shift_ratio": 0.0,
      "brightness_ratio": 0.0,
      "blur_ratio": 0.0,
      "hsv_ratio": 0.0,
      "flip_ratio": 0.0,
      "resize_ratio": 0.0,
      "rotate_ratio": 0.0,
      "crop_ratio": 0.0,
      "det_crop_ratio": 0.0,
      "trans_seq": ["random_brightness", "random_gauss_blur", "random_hsv",
                    "random_hflip", "random_resize", "random_rotate",
                    "random_crop", "resize"]
    },
    "trans_params": {
      "random_shift":{
        "shift_pixel": 2
      },
      "random_pad": {
        "up_scale_range": [1.0, 4.0]
      },
      "random_brightness": {
        "shift_value": 30
      },
      "random_gauss_blur": {
        "max_blur": 4
      },
      "random_hsv": {
        "h_range": [0.7, 1.3],
        "s_range": [0.7, 1.3],
        "v_range": [0.7, 1.3]
      },
      "random_hflip": {
        "swap_pair": []
      },
      "random_resize": {
        "scale_min": 0.75,
        "scale_max": 1.25
      },
      "random_rotate": {
        "rotate_degree": 40
      },
      "random_crop":{
        "crop_size": [1024, 512],
        "method": "grid",
        "grid":[3, 2],
        "center_jitter": 20
      },
      "mean": [0.485, 0.456, 0.406],
      "std": [0.229, 0.224, 0.225]
    },
    "gt": {
      "anchors_list": [
        [[116, 90], [156, 198], [373, 326]],
        [[30, 61], [62, 45], [59, 119]],
        [[10, 13], [16, 30], [33, 23]]
      ],
      "iou_threshold": 0.5
    },
    "details": {
      "color_list": [[255, 170, 30], [0, 0, 70], [244, 35, 232]],
      "name_id_dict": {
        "aeroplane": 1, "bicycle": 2, "bird": 3, "boat": 4, "bottle": 5, "bus": 6, "car": 7,
        "cat": 8, "chair": 9, "cow": 10, "diningtable": 11, "dog": 12, "horse": 13, "motorbike": 14,
        "person": 15, "pottedplant": 16, "sheep": 17, "sofa": 18, "train": 19, "tvmonitor": 20
      },
      "name_seq": ["aeroplane", "bicycle", "bird", "boat", "bottle", "bus", "car", "cat", "chair",
                   "cow", "diningtable", "dog", "horse", "motorbike", "person", "pottedplant",
                   "sheep", "sofa", "train", "tvmonitor"]
    },
    "checkpoints": {
      "checkpoints_name": "yolov3_darknet_voc_det",
      "checkpoints_dir": "checkpoints/det/voc",
      "save_iters": 3000
    },
    "network":{
      "backbone": "darknet53",
      "model_name": "darknet_yolov3",
      "init": "kaiming_normal",
      "stride_list": [32, 16, 8],
      "loss_weights": {
        "coord_loss": 2.5,
        "obj_loss": 1.0,
        "cls_loss": 1.0
      }
    },
    "logging": {
      "logfile_level": "info",
      "stdout_level": "info",
      "log_file": "./log/det/yolov3_darknet_voc_det.log",
      "log_format": "%(asctime)s %(levelname)-7s %(message)s",
      "rewrite": true
    },
    "lr": {
      "base_lr": 0.001,
      "lr_policy": "step",
      "step": {
        "gamma": 0.1,
        "step_size": 20
      }
    },
    "solver": {
      "display_iter": 100,
      "test_interval": 1000,
      "max_epoch": 100
    },
    "optim": {
      "optim_method": "sgd",
      "adam": {
        "betas": [0.9, 0.999],
        "eps": 1e-08,
        "weight_decay": 0.0001
      },
      "sgd":{
        "weight_decay": 0.00004,
        "momentum": 0.9
      }
    },
    "nms": {
      "mode": "union",
      "overlap_threshold": 0.45
    },
    "vis": {
      "obj_threshold": 0.0
    }
}